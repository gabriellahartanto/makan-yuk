<%- include('partials/header', { title: 'Meals' }) %>
  <p><a href="/meals/add">Add Meal</a></p>
  <p><a href="/topup/:id">Top up Money</a></p>
  <table>
    <tr>
      <th>No</th>
      <th>Name</th>
      <th>Price</th>
      <th>Stock</th>
      <th>QR Code</th>
    </tr>
    <% for (let i = 0; i < data.length; i++) { %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= data[i].name %></td>
        <td><%= data[i].price %></td>
        <td><%= data[i].stock %></td>
        <td>INI QR CODE</td>
      </tr>
    <% } %>
  </table>
<%- include('partials/footer') %>